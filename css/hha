  50% {
    -webkit-transform: translate3d(0, -7px, 0);
    transform: translate3d(0, -7px, 0);
  }
  100% {
    -webkit-transform: translate3d(0, -7px, 0) rotate(-45deg);
    transform: translate3d(0, -7px, 0) rotate(-45deg);
  }
}
